      "source": [
        "year = int(input(\"Enter year to check :\"))\n",
        "if year%100==0:\n",
        "  if year%400==0:\n",
        "    print(\"The year=\",year,\"is a leap year\")\n",
        "elif year%4==0:\n",
        "  print(\"The year=\",year,\"is a leap year\")\n",
        "else:\n",
        "  print(\"The year=\",year,\"is not a leap year\")"
      ],
